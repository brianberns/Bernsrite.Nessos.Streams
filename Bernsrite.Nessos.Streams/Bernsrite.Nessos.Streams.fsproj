<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>0.6.0</Version>
    <Authors>Brian Berns</Authors>
    <Description>Extensions for Nessos streams</Description>
    <Copyright>2019</Copyright>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <PackageProjectUrl>https://github.com/brianberns/Bernsrite.Nessos.Streams</PackageProjectUrl>
    <RepositoryUrl>https://github.com/brianberns/Bernsrite.Nessos.Streams</RepositoryUrl>
    <PackageIconUrl>https://avatars2.githubusercontent.com/u/11883138</PackageIconUrl>
  </PropertyGroup>

  <ItemGroup>
    <Compile Include="Streams.fs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Streams" Version="0.4.1" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
